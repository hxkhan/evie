package main

fn main() {
    message := "hello world"
    parts := message.split(" ")
    whole := parts.join("_")

    // hello world -> ["hello", "world"] -> hello_world
    echo message + " -> " + str(parts) + " -> " +  whole

    // hello world -> hello-world
    echo message + " -> " + message.split(" ").join("-")
}