package main imports("io")

fn main() {
    // 1 layer of capture
    var printer := single()
    printer()

    // 2 layers of capture
    getter := double()
    printer = getter()
    printer()
}

fn single() {
    message := "Hello World"

    // printer
    return fn() {
        io.println(message)
    }
}

fn double() {
    message := "Bye World"

    // getter
    return fn() {
        // printer
        return fn() {
            io.println(message)
        }
    }
}