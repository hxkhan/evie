package main

fn dec(id, n) unsynced {
    echo `{id} started`
    while n > 0 {
        n -= 1
    }
    echo `{id} finished`
}

fn main() {
    go dec("a", 100000000)
    go dec("b", 100000000)
    go dec("c", 100000000)
    go dec("d", 100000000)
    go dec("e", 100000000)

    echo "all initiated"
}